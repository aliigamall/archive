doctype 5
include mixins/siteheader.pug
include mixins/sidenav.pug
include mixins/main.pug
html(lang="en")

    head
        include modules/meta.pug
        include modules/styles.pug
        title الأرشيف

    body
        +siteheader('siteheader-publications siteheader-publications--facebook')
        .container-fluid
            .row
                +sidenav('', '', 'sidenav-active', '', '')
                .l-pagecontent
                    +main('أرشيف تويتر', 'pageHeader--facebook', 'card-facebook')
                    include modules/footer.pug
        include modules/modal.pug
        include modules/scripts.pug